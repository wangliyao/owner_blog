<div class="col-md-6">
  <div class="content-box-large">
    <div class="panel-heading">
      <div class="panel-title">补充基本信息</div>
    </div>
    <div class="panel-body">
      <%= form_for(@other, :url => {:action => "create_other"}, :remote => true) do |f| %>
          <div class="form-group">
            <label for="inputEmail3" class="col-sm-2 control-label">姓名</label>

            <div class="col-sm-10">
              <%= f.select :person_id, select_name, {:class => "dropdown"} %>
            </div>
          </div>
          <%= cache ['all',Time.now] do %>
              <div class="form-group">
                <label for="inputPassword3" class="col-sm-2 control-label">选择关联人</label>

                <div class="col-sm-10">
                  <% @all.each do |t| %>
                      <input type="checkbox" id="<%= t.id %>" value="<%= t.id %>" name="material[][code_name]"><%= t.name %> </input>
                  <% end %>
                </div>
              </div>
          <% end %>
          </div>
          <div class="form-group">
            <div class="col-sm-offset-2 col-sm-10">
              <%= f.submit("保存", :class => "btn btn-success") %>
            </div>
          </div>
      <% end %>
      </div>
</div>